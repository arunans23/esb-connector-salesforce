            <file>getBatchRequest.xml</file>
            <description>Get an existing batch request.</description>
        </component>
        <component name="listBatches-bulk">
            <file>listBatches.xml</file>
            <description>Get information about all batches in a job.</description>
        </component>
        <component name="getBulkQueryResults-bulk">
            <file>getBulkQueryResults.xml</file>
            <description>Get the results of the query.</description>
        </component>
    </subComponents>
